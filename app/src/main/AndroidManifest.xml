<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ptit.nttrung.profiletranning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="android.support.multidex.MultiDexApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".createaccount.CreateAccountActivity" />
        <activity android:name=".profilepage.ProfilePageActivity" />
        <activity android:name=".splash.SplashActivity" />
        <activity android:name=".photodetail.PhotoDetailActivity" />
        <activity android:name=".profilesettings.ProfileSettingsActivity" />
        <activity android:name=".profiledetail.ProfileDetailActivity" />
        <activity android:name=".photogallery.PhotoGalleryActivity"></activity>
    </application>

</manifest>